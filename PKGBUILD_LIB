pkgname=levtree
arch=('x86_64')
pkgver=0.1
pkgrel=1
pkgdesc="A small C library for fast fuzzy string search"
license="GPL"
depends=('')
source=('levtree.tar.gz')
md5sums=('f567abda5386de0f06aff8150286286f')

build() {
cd $srcdir/levtree
cmake .
make
}

package()
{
	cd $srcdir/levtree
	make install DESTDIR="$pkgdir"
}
