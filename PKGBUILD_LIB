pkgname=levtree
arch=('x86_64')
pkgver=0.1
pkgrel=1
pkgdesc="A small C library for fast fuzzy string search"
license="GPL"
depends=('')
source=('levtree.tar.gz')
md5sums=('deabb4ed6154f5e9c7e7a02eaa76c2e3')

build() {
cd $srcdir/levtree
qmake levtree.pro
make
}

package()
{

	cd $srcdir/levtree
	make install INSTALL_ROOT="$pkgdir"
}
