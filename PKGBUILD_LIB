pkgname=levtree
arch=('x86_64')
pkgver=0.1
pkgrel=1
pkgdesc="A small C library for fast fuzzy string search"
license="GPL"
depends=('')
source=('levtree.tar.gz')
md5sums=('9731af3b6325f279f2ea8dc3167118b8')

build() {
cd $srcdir/levtree
qmake levtree.pro
make
}

package()
{

	cd $srcdir/levtree
	make install INSTALL_ROOT="$pkgdir"
}
